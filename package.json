{
  "name": "lunis-browser",
  "version": "1.0.0",
  "description": "Lunis Browser - A modern cross-platform web browser built with Electron, TypeScript, and React",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "npm run build && electron .",
    "dev": "npm run build:main && electron . --dev",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "webpack --config webpack.config.js",
    "watch": "webpack --config webpack.config.js --watch",
    "clean": "rimraf dist",
    "package": "npm run build && npx electron-packager . \"Lunis Browser\" --platform=win32 --arch=x64 --out=release --overwrite --asar --icon=icon.ico",
    "package:all": "npm run build && npx electron-packager . \"Lunis Browser\" --all --out=release --overwrite --asar"
  },
  "keywords": [
    "electron",
    "browser",
    "typescript",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "css-loader": "^6.8.1",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-packager": "^17.1.2",
    "html-webpack-plugin": "^5.5.4",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@types/unzipper": "^0.10.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "unzipper": "^0.12.3",
    "uuid": "^9.0.1"
  },
  "build": {
    "appId": "com.electronbrowser.app",
    "productName": "Electron Browser",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "sign": null
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
